{
    "name": "forjix/core",
    "description": "Forjix Framework core components",
    "type": "library",
    "version": "0.1.0",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.2",
        "forjix/support": "^1.0",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Forjix\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forjix\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
